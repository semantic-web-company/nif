<http://example.com#offset_0_39>
        a               nif:OffsetBasedString , nif:Context , nif:Sentence ;
        nif:beginIndex  "0"^^xsd:nonNegativeInteger ;
        nif:endIndex    "39"^^xsd:nonNegativeInteger ;
        nif:referenceContext  <http://example.com#offset_0_39> ;
        nif:word <http://example.com#offset_0_9> , <http://example.com#offset_10_19> , <http://example.com#offset_20_25> , <http://example.com#offset_26_31> , <http://example.com#offset_32_39> ;
        nif:isString    "Christian mentioned tiger shark hunting" .

<http://example.com#offset_0_9>
        a                     nif:OffsetBasedString , nif:Word , nif:NamedEntity ;
        nif:anchorOf          "Christian" ;
        nif:beginIndex        "0"^^xsd:nonNegativeInteger ;
        nif:endIndex          "9"^^xsd:nonNegativeInteger ;
        nif:nextWord          <http://example.com#offset_10_19> ;
        nif:referenceContext  <http://example.com#offset_0_39> ;
        nif:sentence          <http://example.com#offset_0_39> ;
        nif:annotationUnit [
               itsrdf:taConfidence "0.9"^^xsd:decimal ;
               itsrdf:taClassRef dbo:Person ;
               itsrdf:taAnnotatorRef <http://some.ner.tool> ;
        ] .

